{
  "name": "procket/downloader",
  "description": "HTTP file and video downloader.",
  "keywords": [
    "file downloader",
    "video downloader"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ext-iconv": "*",
    "ext-json": "*",
    "norkunas/youtube-dl-php": "^2.6",
    "illuminate/filesystem": "^11.0",
    "illuminate/http": "^11.0",
    "symfony/console": "^7.1"
  },
  "autoload": {
    "psr-4": {
      "Procket\\Downloader\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
