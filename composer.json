{
  "name": "pocket/downloader",
  "description": "HTTP file and video downloader.",
  "keywords": [
    "file downloader",
    "video downloader"
  ],
  "license": "MIT",
  "repositories": [
    {
      "type": "path",
      "url": "../*"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-iconv": "*",
    "ext-json": "*",
    "norkunas/youtube-dl-php": "^2.6",
    "pocket/framework": "@dev"
  },
  "autoload": {
    "psr-4": {
      "Pocket\\Downloader\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
